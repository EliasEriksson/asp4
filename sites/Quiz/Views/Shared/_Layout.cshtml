<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8"/>
    <meta name="viewport" content="width=device-width, initial-scale=1.0"/>
    <title>@ViewData["Title"] - Quiz</title>
    <base href="/aspdotnet/moment4/static/"/>
    <link rel="icon" type="image/png" href="/aspdotnet/moment4/static/favicon.png">
    <link rel="stylesheet" href="css/site.css" asp-append-version="true"/>
    @if (ViewData["javascript"] != null)
    {
        @foreach (var src in (List<string>)ViewData["javascript"]!)
        {
            <script type="module" src="@src"></script>
        }
    }
</head>
<body>
<header>
    <nav>
        <ul>
            <li><a asp-controller="Root" asp-action="Index">Home</a></li>
            <li><a asp-controller="Quiz" asp-action="Index">Quiz</a></li>
        </ul>
        <partial name="_LoginPartial"/>
    </nav>
</header>

<main>
    @RenderBody()
</main>

<footer>
</footer>
<script src="lib/jquery/dist/jquery.min.js"></script>
<script src="lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
<script src="js/site.js" asp-append-version="true"></script>
@await RenderSectionAsync("Scripts", required: false)
</body>
</html>